<h1>Home Page</h1>

<% @songs.each do |song|%>
  <div class="song">
    <h3><a href="/songs/<%= song.id %>"><%= song.title %></a> <small>(by: <%= song.author %>)</small></h3>
    <p>
      <a href="<%= song.url %>" target="_blank"><%= song.url %></a> 
    </p>
  </div>
<% end %>

<a href="/songs/new">Add A Song</a>